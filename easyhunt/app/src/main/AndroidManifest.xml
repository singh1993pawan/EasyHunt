<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.easyhunt">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/easyhunt"
        android:label="@string/app_name"
        android:roundIcon="@drawable/easyhunt"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />
        <activity android:name=".SelectLanguage" />
        <activity android:name=".WebLogin" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".ChangePassword" />
        <activity android:name=".NewCustomer" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegistrationActivity"></activity>
    </application>

</manifest>